class Point {
private:
  float x,y;
public:
  Point(float ux,float uy) { x = ux; y = uy; };
  // other methods omitted
};
  /* ... */
int main() {
  Set set;

  set.add( Point(1.0,1.0) );
  set.add( Point(2.0,2.0) );
  cout << "Number of points: " << set.cardinality() << '\n';

  Point p3(4.0,5.0);
  set.add( p3 );
  cout << "Number of points: " << set.cardinality() << '\n';

  return 0;
}
