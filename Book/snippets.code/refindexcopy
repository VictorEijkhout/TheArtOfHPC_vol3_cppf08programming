// /getindex1.cpp
class vector10 {
private:
  int array[10];
public:
    /* ... */
// /getindex1.cpp
  int operator()(int i) {
    return array[i];
  };
  int operator[](int i) {
    return array[i];
  };
};
    /* ... */
// /getindex1.cpp
  vector10 v;
  cout << v(3) << '\n';
  cout << v[2] << '\n';
  /* compilation error: v(3) = -2; */
